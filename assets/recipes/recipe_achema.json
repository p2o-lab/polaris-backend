            },
            {
              "name": "Empty_VolFlow",
              "value": 3
            }
          ]
        }
      ],
      "transitions": [
        {
          "condition": {
            "type": "variable",
            "module": "React",
            "dataStructure": "MCL01_AnaView",
            "variable": "V",
            "operator": "<=",
            "value": "0.1"
          },
          "next_step": "Running.Vorlegen"
        }
      ]
    },
    {
      "name": "Running.Vorlegen",
      "operations": [
        {
          "module": "Dose",
          "service": "DoseAmount",
          "command": "start",
