            {
              "name": "RampEndTemperature",
              "value": 20
            }
          ]
        }
      ],
      "transitions": [
        {
          "next_step": "Running.Aufheizen.Stop",
          "condition": {
            "type": "variable",
            "module": "Temper",
            "variable": "TI",
            "operator": ">=",
            "value": 20
          }
        }
      ]
    },
    {
      "name": "Running.Aufheizen.Stop",
      "operations": [
        {
          "module": "Temper",
          "service": "Tempering",
          "command": "complete"
        },
        {
          "module": "React",
          "service": "Stir_Service",
          "command": "complete"
        }
      ],
      "transitions": [
        {
          "next_step": "Shutdown.Entleeren.Start",
          "condition": {
            "type": "and",
            "conditions": [
              {
                "type": "state",
                "module": "Temper",
                "service": "Tempering",
                "state": "idle"
              },
              {
                "type": "state",
                "module": "React",
                "service": "Stir_Service",
                "state": "idle"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "Shutdown.Entleeren.Start",
      "operations": [
        {
          "module": "Dose",
          "service": "Fill",
          "command": "start"
        }
      ],
      "transitions": [
        {
          "next_step": "Shutdown.Entleeren.Running",
          "condition": {
            "type": "state",
            "module": "Dose",
            "service": "Fill",
            "state": "running"
          }
        }
      ]
    },
    {
      "name": "Shutdown.Entleeren.Running",
      "operations": [
        {
          "module": "React",
          "service": "Empty_Service",
          "command": "start"
        }
      ],
      "transitions": [
        {
          "next_step": "Shutdown.Entleeren.Stop",
          "condition": {
            "type": "state",
            "module": "React",
            "service": "Empty_Service",
            "state": "completed"
          }
        }
      ]
    },
    {
      "name": "Shutdown.Entleeren.Stop",
      "operations": [
        {
          "module": "Dose",
          "service": "Fill",
          "command": "complete"
        }
      ],
      "transitions": [
        {
          "next_step": "finished",
          "condition": {
            "type": "state",
            "module": "Dose",
            "service": "Fill",
            "state": "idle"
          }
        }
      ]
    }
