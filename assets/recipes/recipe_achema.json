              "name": "SetVolume",
              "value": 1
            },
            {
              "name": "SetDuration",
              "value": 30
            }
          ]
        },
        {
          "module": "React",
          "service": "Fill_Service",
          "command": "start",
          "parameters": [
            {
              "name": "Fill_Level_Max",
              "value": 1.5
            }
          ]
        },
        {
          "module": "React",
          "service": "Stir_Service",
          "strategy": "Stir_Continous",
          "command": "start",
          "parameter": [
