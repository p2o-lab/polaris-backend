            {
              "name": "Stir_Rotation",
              "value": 200
            }
          ]
        },
        {
          "module": "React",
          "service": "Aerate_Service",
          "strategy": "Aerate_Continous",
          "command": "start"
        },
        {
          "module": "Temper",
          "service": "Tempering",
          "strategy": "SetpointRampTempering",
          "command": "start",
          "parameter": [
            {
              "name": "RampDuration",
              "value": 80
            },
            {
              "name": "RampEndTemperature",
              "value": 15
            }
          ]
        }
      ],
      "transitions": [
        {
          "next_step": "Running.Ruhen",
          "condition": {
            "type": "and",
            "conditions": [
              {
                "type": "state",
                "module": "Dose",
                "service": "DoseAmount",
                "state": "idle"
              },
              {
                "type": "variable",
                "module": "Temper",
                "variable": "TI",
                "operator": "<=",
                "value": 15
              }
