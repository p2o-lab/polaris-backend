{
  "name": "@p2olab/polaris-backend",
  "version": "3.0.0-alpha0.1",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "description": "Server backend for POL recipe execution engine",
  "author": "P2O-Lab <p2o-lab@mailbox.tu-dresden.de>",
  "repository": "https://github.com/p2o-lab/polaris-backend.git",
  "homepage": "https://github.com/p2o-lab/polaris-backend",
  "license": "MIT",
  "contributors": [
    "Markus Graube",
    "Sebastian Heinze",
    "Christoph Jesaja Heidelbach <christoph.jesaja.heidelbach@gmail.com>",
    "Julius Lorenz <julius.lorenz@tu-dresden.de>",
    "Tuan Son Dinh <tuansondinh96@gmail.com"
  ],
  "scripts": {
    "apidoc": "npm run clrApiDoc && apidoc -i src/server/ -o apidoc",
    "build": "tsc",
    "clrApiDoc": "run-script-os",
    "clrApiDoc:linux": "rm -rf apidoc",
    "clrApiDoc:win32": "(IF EXIST apidoc (rd /s /q apidoc))",
    "clrBuild": "run-script-os",
    "clrBuild:linux": "rm -rf build",
    "clrBuild:win32": "(IF EXIST build (rd /s /q build))",
    "clrTest": "run-script-os",
    "clrTest:linux": "rm -rf .nyc_output && coverage",
    "clrTest:win32": "(IF EXIST .nyc_output (rd /s /q .nyc_output)) && (IF EXIST coverage (rd /s /q coverage))",
    "clrTypeDoc": "run-script-os",
    "clrTypeDoc:linux": "rm -rf typedoc",
    "clrTypeDoc:win32": "(IF EXIST typedoc (rd /s /q typedoc))",
    "dist": "node ./build/index.js",
    "doc": "npm run apidoc && npm run typedoc",
    "lint": "eslint -f pretty --ext .ts --ignore-pattern \"**/*.spec.ts\" --ignore-pattern \"**/*.mockup.ts\" src",
    "prebuild": "npm run clrBuild && npm run lint",
    "start": "ts-node ./src/index.ts",
    "test": "nyc mocha --exit --require ts-node/register --recursive \"./src/**/*.spec.ts\"",
    "typedoc": "npm run clrTypeDoc && typedoc --mode file --module commonjs --out typedoc/ --target ES2017 --excludePrivate --readme none src/"
  },
  "bin": {
    "polaris-backend": "./bin/polaris-backend"
  },
  "dependencies": {
    "@p2olab/pimad-core": "0.0.1alpha16",
    "@p2olab/pimad-types": "1.0.1-alpha1.0",
    "@p2olab/polaris-interface": "3.0.0-alpha13",
    "@types/adm-zip": "^0.4.34",
    "@types/pino": "^6.3.6",
    "@types/promise-timeout": "^1.3.0",
    "@types/uuid": "^8.3.0",
    "adm-zip": "^0.5.5",
    "assign-deep": "^1.0.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "cors": "^2.8.5",
    "expr-eval": "^2.0.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "json-parse-better-errors": "^1.0.2",
    "multer": "^1.4.2",
    "node-opcua": "^2.31.0",
    "node-pid-controller": "^1.0.1",
    "promise-timeout": "^1.3.0",
    "timeout-as-promise": "^1.0.0",
    "typescript-logging": "^0.6.4",
    "underscore": "^1.10.2",
    "uuid": "^8.1.0",
    "ws": "^7.5.0",
    "xstate": "^4.23.1",
    "yn": "^3.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.0.9",
    "@types/underscore": "^1.10.7",
    "@types/uuid": "^8.0.0",
    "@types/ws": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "apidoc": "^0.23.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-tsdoc": "^0.2.4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "run-script-os": "^1.1.1",
    "strict-event-emitter-types": "^2.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.6",
    "xstate": "^4.23.1"
  }
}
