@startuml
'https://plantuml.com/class-diagram
!include anaManInt/AnaManInt.puml

!include ../../../baseFunction/scaleSettings/ScaleSettings.puml
!include ../../../baseFunction/unitSettings/UnitSettings.puml
!include ../../../baseFunction/valueLimitation/ValueLimitation.puml

class AnaMan{
    + readonly communication!: AnaManRuntime
    + readonly scaleSettings: ScaleSettings
    + readonly unitSettings: UnitSettings
    + readonly valueLimitation: ValueLimitation
}

AnaMan *- AnaManSpec
AnaManSpec ..> AnaManMockup : <<use>>
AnaMan *- AnaManMockup
AnaMan <|-- AnaManInt

AnaMan *- AnaManRuntime
AnaMan *- ScaleSettings
AnaMan *- UnitSettings
AnaMan *- ValueLimitation

@enduml
