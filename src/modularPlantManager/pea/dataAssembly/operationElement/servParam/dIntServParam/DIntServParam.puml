@startuml
'https://plantuml.com/class-diagram
!include ../ServParam.puml
!include ../../../baseFunction/scaleSettings/ScaleSettings.puml
!include ../../../baseFunction/unitSettings/UnitSettings.puml
!include ../../../baseFunction/valueLimitation/ValueLimitation.puml

interface DIntServParamRuntime{
	+ VExt: DataItem<number>
	+ VOp: DataItem<number>
	+ VInt: DataItem<number>
	+ VReq: DataItem<number>
	+ VOut: DataItem<number>
	+ VFbk: DataItem<number>

}

class DIntServParam{
    + readonly dataItems!: DIntServParamRuntime
    + scaleSettings: ScaleSettings
    + unitSettings: UnitSettings
    + valueLimitation: ValueLimitation

    + DIntServParam(options: DataAssemblyModel, connectionHandler: ConnectionHandler, initial = false)
    # initializeBaseFunctions()
}
DIntServParam *- DIntServParamSpec
DIntServParamSpec ..> DIntServParamMockup : <<use>>
DIntServParam *- DIntServParamMockup

DIntServParam *- DIntServParamRuntime
DIntServParam *- ScaleSettings
DIntServParam *- UnitSettings
DIntServParam *- ValueLimitation
@enduml
